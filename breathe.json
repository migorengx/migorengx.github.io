{
    "title": "Breathe",
    "verses": [
      {
        "id": 1,
        "lines": [
          "In this melody",
          "Find your peace",
          "Let the rhythm put your soul at ease",
          "With every note",
          "Let your worries fall",
          "Breathe in the calm",
          "Just let go of it all"
        ]
      },
      {
        "id": 2,
        "lines": [
          "In the minor key of life's symphony",
          "We battle anxiety",
          "Set our minds free",
          "Like a gentle breeze",
          "Let the music guide",
          "Release your fears",
          "Find solace inside"
        ]
      },
      {
        "id": 3,
        "lines": [
          "Breathe",
          "Breathe",
          "Let it out",
          "Let the waves of serenity wash away your doubt",
          "Breathe",
          "Find your way",
          "In this harmonious rhythm",
          "We'll conquer each day"
        ]
      },
      {
