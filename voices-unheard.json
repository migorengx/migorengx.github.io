{
    "title": "Voices Unheard",
    "verses": [
      {
        "id": 1,
        "lines": [
          "Frustration building",
          "Cannot contain",
          "Injustice rising",
          "Causing so much pain",
          "The gap between the poor and rich so clear",
          "Voices unheard",
          "We gotta persevere"
        ]
      },
      {
        "id": 2,
        "lines": [
          "In this world of chaos",
          "We seek some peace",
          "Society overlooks those who need release",
          "The broken system",
          "A cycle that won't end",
          "Our hearts are heavy",
          "But we still pretend"
        ]
      },
      {
        "id": 3,
        "lines": [
          "Oh",
          "The struggle is so real",
          "Can't deny the way we feel",
          "Through the static",
          "We'll make a sound",
          "Voices unheard",
          "We won't back down"
        ]
      }
    ]
  }